{
  "manifest_version": 2,
  "name": "ManageWP 2 Cloudflare",
  "version": "0.1",

  "content_scripts": [
	  {
	    "matches": [
	      "https://dash.cloudflare.com/*/*/firewall/tools"
	    ],
	    "js": ["content.js"]
	  }
	],

	  "browser_action": {
	  "default_icon": "icon.png"
	},

	"background": {
  		"scripts": ["background.js"]
	}
}